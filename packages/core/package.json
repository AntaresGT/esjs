{
  "name": "@es-js/core",
  "version": "0.1.0-alpha.7",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "esjs",
    "lenguaje-programacion",
    "espa√±ol",
    "spanish"
  ],
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@es-js/esbabel": "workspace:0.0.1-alpha.0",
    "@types/node": "^20.9.0",
    "eslint": "8.55.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^5.1.1",
    "vitest": "^1.2.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./keywords": {
      "types": "./dist/keywords.d.ts",
      "import": "./dist/keywords.mjs",
      "require": "./dist/keywords.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts",
        "./dist/keywords.d.ts",
        "./dist/utils.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ]
