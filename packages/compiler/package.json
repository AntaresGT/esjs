{
  "name": "@es-js/compiler",
  "version": "0.0.3",
  "scripts": {
    "dev": "vite",
    "build": "unbuild",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.2",
    "@babel/types": "^7.22.5",
    "@es-js/core": "workspace:^",
    "@types/escodegen": "^0.0.7",
    "@types/prettier": "^2.7.3",
    "@vue/compiler-sfc": "^3.3.4",
    "escodegen": "^2.1.0",
    "eslint": "8.22.0",
    "espree": "^9.6.1",
    "prettier": "^2.8.8",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./orchestrator": {
      "types": "./dist/orchestrator.d.ts",
      "import": "./dist/orchestrator.mjs",
      "require": "./dist/orchestrator.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts",
        "./dist/orchestrator.d.ts"
      ]
    }
  },
  "files": [
    "/dist"
  ]
}
