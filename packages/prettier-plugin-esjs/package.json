{
	"name": "prettier-plugin-esjs",
	"version": "0.0.4",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "pkgroll",
		"lint": "eslint . && prettier . --check",
		"lint:fix": "eslint . --fix && prettier . --write",
		"start": "tsx src/index.ts",
		"test": "vitest",
		"typecheck": "tsc --noEmit",
		"watch": "pkgroll --watch"
	},
	"devDependencies": {
		"@es-js/core": "workspace:*",
		"@prettier/sync": "^0.3.0",
		"@types/node": "^18.17.1",
		"eslint": "8.22.0",
		"pkgroll": "^1.10.0",
		"prettier": "^3.0.3",
		"tsx": "^3.12.7",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vitest": "^0.34.6"
	},
	"peerDependencies": {
		"prettier": "^3.0.0"
	}
}
