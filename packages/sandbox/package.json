{
  "name": "@es-js/sandbox",
  "version": "0.0.5-alpha.10",
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm tailwind:build",
    "test": "vitest",
    "test:run": "vitest run",
    "tailwind:dev": "tailwindcss -i ./src/style.css -o ./dist/style.css --watch",
    "tailwind:build": "tailwindcss -i ./src/style.css -o ./dist/style.css --minify"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.1",
    "autoprefixer": "^10.4.16",
    "eruda": "^3.0.1",
    "eslint": "8.22.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^1.7.3"
  },
  "peerDependencies": {
    "@es-js/compiler": "workspace:^"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./render": {
      "types": "./dist/render.d.ts",
      "import": "./dist/render.js",
      "require": "./dist/render.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts",
        "./dist/render.d.ts"
      ]
    }
  },
  "files": [
    "/dist"
  ]
}
